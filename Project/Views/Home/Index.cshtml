@{
    ViewBag.Title = "Home Page";

}




<section class="section columns">
    <div id="draggable" class="column is-one-third" style="position: absolute">
        <div id="weatherContent"></div>

    </div>
    <div id="newsTicker" style="position: fixed; bottom: 0; width: 100%;" class="column is-one-third"></div>
</section>


<script>var initialWeatherData = @Html.Raw(Json.Encode(Model.Weather));</script>
<script>var initialNewsData = @Html.Raw(Json.Encode(Model.News))</script>
<script src="@Url.Content("~/Resources/WeatherComponent/weather.jsx")"></script>
<script type="module" src="@Url.Content("~/Resources/NewsTicker/News.jsx")"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js" type="text/javascript"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js" type="text/javascript"></script>
<script>
    $(function () {
        $("#draggable").draggable();
    });
</script>